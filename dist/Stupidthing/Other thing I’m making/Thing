;}},600);overlay.addEventListener('click',(e)=>{if(e.target===overlay){overlay.remove();window.cyberMenuActive=false;}});const closeBtn=document.getElementById('cyber-close');if(closeBtn){closeBtn.addEventListener('click',()=>{overlay.remove();window.cyberMenuActive=false;});}document.querySelectorAll('.cyber-item').forEach(item=>{item.addEventListener('click',function(){document.querySelectorAll('.cyber-item').forEach(i=>i.classList.remove('active'));this.classList.add('active');const section=this.getAttribute('data-section');if(section==='games'){loadGames();}else if(section==='settings'){renderSettings();iframeContainer.style.display='none';}else{renderPlaceholder(this.textContent);iframeContainer.style.display='none';}});});}catch(err){console.error('fatal:',err);window.cyberMenuActive=false;}})();
